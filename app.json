{
  "expo": {
    "name": "Todo Cat",
    "slug": "todo-cat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "todocat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "タスク管理と猫育成を組み合わせた、楽しく続けられるTodoアプリ",
    "primaryColor": "#4A90E2",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.beachone1155.todocat",
      "buildNumber": "1",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ],
        "CFBundleLocalizations": [
          "ja"
        ],
        "CFBundleDevelopmentRegion": "ja_JP"
      }
    },
    "android": {
      "package": "com.beachone1155.todocat",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f4b2d348-7b8d-4354-a183-efd964055564"
      }
    },
    "owner": "beachone1155"
  }
}
